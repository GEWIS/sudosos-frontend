{
  "name": "sudosos-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dashboard:dev": "yarn dev",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "format": "prettier --ignore-path .gitignore --check .",
    "format-fix": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "@stripe/stripe-js": "^5.2.0",
    "@sudosos/sudosos-frontend-common": "workspace:^",
    "@sudosos/themes": "workspace:^",
    "@tailwindcss/vite": "^4.1.4",
    "@vee-validate/rules": "^4.14.1",
    "@vee-validate/yup": "^4.14.1",
    "marked": "^15.0.1",
    "vee-validate": "^4.14.1",
    "vue-i18n": "^9.14.1",
    "vue3-pdf-app": "^1.0.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "eslint": "^9.24.0",
    "vue-tsc": "^2.2.8"
  }
}
