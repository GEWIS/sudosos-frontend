{
  "name": "sudosos-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dashboard:dev": "yarn dev",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cts,.mts,.json --ignore-path .gitignore",
    "lint-fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json --fix --ignore-path .gitignore --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@stripe/stripe-js": "^5.2.0",
    "@sudosos/sudosos-frontend-common": "workspace:^",
    "@vee-validate/rules": "^4.14.1",
    "@vee-validate/yup": "^4.14.1",
    "marked": "^15.0.1",
    "vee-validate": "^4.14.1",
    "vue-i18n": "^9.14.1",
    "vue3-pdf-app": "^1.0.3",
    "yup": "^1.4.0"
  }
}
